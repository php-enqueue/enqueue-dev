{
    "name": "enqueue/redis",
    "type": "library",
    "description": "Message Queue Redis Transport",
    "keywords": ["messaging", "queue", "redis"],
    "homepage": "https://enqueue.forma-pro.com/",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "queue-interop/queue-interop": "^0.7",
        "enqueue/dsn": "0.9.x-dev",
        "ramsey/uuid": "^3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4.0",
        "predis/predis": "^1.1",
        "enqueue/test": "0.9.x-dev",
        "enqueue/null": "0.9.x-dev",
        "queue-interop/queue-spec": "^0.6"
    },
    "support": {
        "email": "opensource@forma-pro.com",
        "issues": "https://github.com/php-enqueue/enqueue-dev/issues",
        "forum": "https://gitter.im/php-enqueue/Lobby",
        "source": "https://github.com/php-enqueue/enqueue-dev",
        "docs": "https://github.com/php-enqueue/enqueue-dev/blob/master/docs/index.md"
    },
    "autoload": {
        "psr-4": { "Enqueue\\Redis\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "suggest": {
        "ext-redis": "If you'd like to use phpredis extension."
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.9.x-dev"
        }
    }
}
